version: '3.8'

services:
  slm-dashboard-app:
    # Use your Docker image name here
    image: goswamirohit/slmcmsdashboard:v5
    container_name: slm-dashboard-app
    restart: unless-stopped
    ports:
      - "3000:3000"
    # This is the key line that tells Docker to read the variables from the .env file
    env_file:
      - .env.production